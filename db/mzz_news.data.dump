INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Уэббер призывает объединится ради безопасности', 'admin', 'Марк Уэббер, представитель Ассоциации гонщиков \"Гран-при\", ждет от команд единой позиции по поводу повышения уровни безопасности во время тестов.<br>Пока гонщикам просто везет, но это не может продолжаться вечно\", - сказал Марк Уэббер.<br>', '1');

