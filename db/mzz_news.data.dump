INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Организаторы ЧМ советуют не покупать билеты в интернете', 'admin', 'Глава футбольного союза Германии Тео Цванцигер призвал футбольных болельщиков не покупать билеты на матчи ЧМ-2006 в сети интернет.<br>Организаторы ЧМ-2006, чтобы предотвратить попадание на стадионы хулиганов, а также затруднить деятельность спекулянтам, сделали все билеты именными.<br>', '1');

