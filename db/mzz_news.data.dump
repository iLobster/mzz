INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Последняя капля', 'admin', 'Количество рекламы, повествующей о том, что в среду в Сен-Дени сыграют \"Барса\" и \"Арсенал\", превышало всякие возможные разумные пределы.<br>\"Меня не надо жалеть, - отвечает Альберт устами Аранчи Санчеса, - я \"Ролан Гаррос\" уже выигрывал, а вот \"Барса\" в Париже Лигу чемпионов еще никогда.<br>', '1');

