INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Сотни миллионов рублей в год позволят сэкономить Хабаровску новые энергосберегающие технологии', 'admin', ' ... по установке приборов учёта тепловой энергии, горячей и холодной воды, шаровых кранов, регуляторов температуры и давления, модернизированы элеваторные узлы, восстановлена ... <br>К сожалению, жилищные организации не заинтересованы в энергосбережении из-за отсутствия стимулирования за снижение потерь энергоресурсов при обслуживании жилищного фонда.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
