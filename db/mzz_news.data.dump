INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Матч "Спартак"-ЦСКА будут охранять 3 тысячи милиционеров', 'admin', '3 тысячи сотрудников милиции будут обеспечивать безопасность на матче финала Кубка России по футболу, который пройдет в субботу днем на стадионе \"Лужники\" в Москве ... <br>Перед началом матча, по словам Гильдеева, стадион обследуют 20 служебных собак, а на случай массовых беспорядков у стадиона будет дежурить резерв из 303 бойцов ОМОНа.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Балахнин поставил "Ростову" тройку', 'admin', 'Перед перерывом на чемпионат мира наши футболисты сгоняли ничью с \"Зенитом\" Матчем в Санкт-Петербурге \"Ростов\" завершал промежуточный этап чемпионата России, после ... <br>Пропустив в первом тайме гол с углового от викинга Эрика Хагена, во второй половине встречи вышедший на замену нападающий Исо Каньенда забил гол-близнец в ворота ... <br>', '1');

