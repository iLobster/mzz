INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( '"Эдмонтон ойлерс" в полуфинале', 'admin', 'Команда \"Эдмонтон ойлерс\" стала последним - четвертым - полуфиналистом розыгрыша Кубка Стэнли.<br>Она вышла в этот этап турнира в среду, одержав победу 2:0 в шестом матче серии против \"Сан-Хосе шаркс\" и завершив серию со счетом 4:2. В полуфинале \"Ойлерс\" встретятся с ... <br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
