INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'В Москве построят всероссийский музей спорта', 'admin', 'Здание площадью 8 тыс кв метров будет располагаться около спорткомплекса ЦСКА, а его фасад будет обращен на проектируемый дублер Ленинградского проспекта , - сообщил ... <br>По его словам, экспозиция будущего музея не будет ограничиваться набором хоккейных шлемов и футбольных маек известных спортсменов, а станет своего рода интерактивным ... <br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Сборная России потерпела обидное поражение от чехов', 'admin', 'Кроме чехов это канадцы, в четвертьфинале сломившие сопротивление словаков, шведы, разгромившие американцев, и финны, оказавшиеся сильнее белорусов.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Челябинскому "Трактору" вручили золотые медали', 'admin', 'Сегодня в администрации Челябинской области состоялось награждение команды \"Трактор\" золотыми медалями первенства России по хоккею в высшей лиге.<br>Медали получили все хоккеисты челябинского \"Трактора\" за исключением легионеров, находящихся в отпусках, а также тренерский штаб и работники администрации клуба.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Куляш: Нам просто не повезло', 'admin', 'Защитник сборной России и московского Динамо Денис Куляш после поражения в четвертьфинале сказал, что виновных искать не надо:<br>Здесь, в Латвии, мы сделали все, что могли - бились до последних секунд.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Михайлов: "С этой сборной было приятно работать"', 'admin', 'Тренер сборной России Борис Михайлов считает, что для победы в четвертьфинале чемпионата мира над чехами необходимо было забросить еще одну шайбу в первом периоде.<br>Однако есть и минусы: некоторые игроки по непонятным причинам вызов в сборную проигнорировали, - продолжил Михайлов. <br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Хейда не хотел играть с Россией на ЧМ', 'admin', 'Защитник сборной Чехии и подмосковного \"Химика\" Ян Хейда в интервью радиостанции \"Маяк\" заявил, что его команда не выбирала себе соперника по четвертьфиналу на чемпионате ... <br>Напомним, накануне чехи выиграли в овертайме у России и вышли в полуфинал, где теперь встретятся с командой Финляндии.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Siemens изготовит для РЖД специальные скоростные поезда', 'admin', 'Компания \"Российские железные дороги\" и германский концерн Siemens подписали 19 мая договор на поставку в Россию скоростных поездов типа Velaro RUS на сумму около 600 млн ... <br>Подвижной состав рассчитан на принятую в России широкую колею (1520 мм) и будет на целых 33 см шире используемых в Германии.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Власти Ленобласти рассмотрят возможность создания туристско-рекреационных зон', 'admin', ' ... развития и туризма правительства Ленинградской области будут в ближайшее время изучать положения принятого закона о туристско-рекреационных особых экономических зонах.<br>Напомним, сегодня Государственная дума РФ приняла в третьем чтении закон о туристско-рекреационных особых экономических зонах.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'В Нижегородской области посеяно почти 90% яровых культур', 'admin', 'В Нижегородской области по состоянию на 18 мая яровые культуры посеяны на площади свыше 435 тыс. га (89%), в том числе числе яровые зерновые - 344 тыс. га (более 95%)<br>Сахарная свекла посеяна на площади 12,4 тыс. га (более 99% от плана), льна посеяно более 1100 га (свыше 50%), кукурузы - 7 тыс. га (42%), рапса - 8,6 тыс. га (96%)<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
