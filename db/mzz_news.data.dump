INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Mittal Steel начала поглощение крупнейшей сталелитейной компании', 'admin', 'Индийская компания Mittal Steel официально начала недружественное поглощение другого стального гиганта Arcelor.<br>Сумма, заявленная компанией Лакшми Миттала на 10 процентов ниже рыночной капитализации Arcelor.<br>', '1');

