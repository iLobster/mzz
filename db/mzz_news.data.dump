INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Комментарии Гринспена оказали давление на доллар', 'admin', 'На азиатских торгах курс пары доллар/йена снизился до 110.66 после комментарием бывшего главы ФРС.<br>Алан Гринспен сказал, что наблюдаемое в США замедление на рынке труда затронет расходы, а это является прямым фактором паузы в цикле повышения ставок.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
