INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Самая высокая женщина планеты попала в больницу с подозрением на опухоль мозга', 'admin', '36-летняя китаянка Яо Дэфэнь, считающаяся самой высокой женщиной в мире, на днях была доставлена в шанхайскую больницу \"Жуйцзинь\" для обследования гипофиза.<br>Газета \"Жэньминь жибао\", сообщая об этом случае, пишет, что девушка, страдающая гигантизмом, на 10 см выше, чем ее соотечественник Яо Мин, знаменитый китайский ... <br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Баскетболисты "Спартака-Приморья" впервые за сезон победили в Самаре', 'admin', 'Важную выездную победу одержали приморские баскетболисты над \"ЦСК ВВС - Самара\".<br>Как сообщает \"Восток-Медиа\" со ссылкой на официальный сайт БК \"Спартак-Приморье\", приморцы впервые переиграли \"ЦСК ВВС-Самара\" на их площадке 96:90.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
