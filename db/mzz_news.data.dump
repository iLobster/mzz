INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Арсен Венгер: красная карточка сыграла главную роль', 'admin', 'Поздравляю \"Барселону\" и моих футболистов с прекрасным европейским сезоном и отличной игрой сегодня вечером.<br>После удаления Йенса Леманна нам пришлось 70 минут играть в меньшинстве против команды, которая очень хорошо держит мяч.<br>', '1');

