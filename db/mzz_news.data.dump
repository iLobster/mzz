INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'В июне ЦСКА подпишет новый контракт с баскетбольной Евролигой', 'admin', 'Об этом в пятницу заявил президент ЦСКА Сергей Кущенко на церемонии чествования армейцев министром обороны Сергеем Ивановым в связи с победой в Евролиге.<br>\"Контракт уже приготовлен, а его подписание состоится в июне на ассамблее (Евролиги) в Барселоне, - сообщил Кущенко в интервью РИА Новости. <br>', '1');

