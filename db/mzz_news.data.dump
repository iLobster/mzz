INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Система безопасности на автозаправочной станции в Воронеже', 'admin', 'Развернутый на объекте комплекс безопасности поддерживает 10 цветных телекамер высокого разрешения, а также микрофоны для записи аудиоинформации на кассовых узлах.<br>Для настройки системы кассового контроля была произведена стыковка ПО \"POS-Интеллект\" с кассовым ПО компании \"Сибинтек\" по XML-протоколу.<br>', '1');

