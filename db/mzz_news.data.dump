INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Дети военнослужащих внутренних войск МВД России отдохнут в Греции и Крыму', 'admin', 'Главным командование внутренних войск МВД России в летнем сезоне 2006 года организуется оздоровительный отдых детей военнослужащих внутренних войск МВД России в Греции и ... <br>В 2006 г. для детей военнослужащих внутренних войск МВД России закуплено 800 путевок.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Защитник "Далласа" дисквалифицирован на один матч', 'admin', 'Защитник \"Далласа\" Джейсон Терри из-за дисквалификации пропустит шестой матч серии с \"Сан-Антонио\".<br>Ассоциация наказала баскетболиста за то, что во время предыдущей встречи тот в борьбе за мяч ударил кулаком своего бывшего партнера по команде Майкла Финли.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Выездная победа "Сибирьтелеком-Локомотива"', 'admin', 'Вчера в рамках турнира за 9 - 13 места XII Чемпионата России по баскетболу среди мужских клубов Суперлиги \"А\" сургутский \"Университет-Югра\" принимал на своей площадке ... <br>Новосибирские баскетболисты уверенно начали встречу и в первой десятиминутке сумели не просто повести в счете, а значительно выйти вперед (34:16).<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
