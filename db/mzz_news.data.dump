INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Прокат автомобилей на курортах Европы опасен для жизни', 'admin', 'К примеру, на турецком курорте Алания фирма Avsar Rent Car & Motorbike предлагает напрокат четырнадцатилетний внедорожник Suzuki Samurai со скрученным до 5000 км ... <br>Основные пункты, по которым нещадно критикуются прокатные автомобили, - изношенные или поврежденные покрышки, плохие тормоза и неисправное рулевое управление.<br>', '1');

