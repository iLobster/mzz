INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( '"КазМунайГаз" подвел итоги деятельности в январе-апреле 2006 г', 'admin', 'Объем переработки нефти на Атырауском нефтеперерабатывающем заводе в январе-апреле 2006 г. составил 1 млн 150 тыс. тонн, что на 8,3% выше аналогичного показателя прошлого ... <br>Объем транспортировки нефти по системе магистральных нефтепроводов АО \"КазТрансОйл\" за четыре месяца 2006 года составил 13 млн.<br>', '1');

INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( '"Славнефть" и Администрация Ярославской области подписали Соглашение о сотрудничестве на 2006 год', 'admin', '17 мая 2006 года в Москве Президент ОАО \"НГК \"Славнефть\" Юрий Суханов и губернатор Ярославской области Анатолий Лисицын подписали Дополнение к Соглашению о сотрудничестве ... <br>Стороны договорились, что размер налоговых отчислений предприятий \"Славнефти\" в консолидированный бюджет Ярославской области составит в 2006 году не менее 2 млрд. рублей.<br>', '1');

