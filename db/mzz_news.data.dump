INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'Фрадков целится в десятку', 'admin', 'По словам Михаила Фрадкова, в 2006-2009 годах удельный вес инвестиционных расходов возрастет с 1,9% ВВП в 2005 году до 2,1-2,4% ВВП.<br>Правительство рассчитывает удержать инфляцию в 2006 году в пределах 8-9%. И Михаил Фрадков подтвердил актуальность этой задачи на заседании Межгосударственного совета ... <br>', '1');

