INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'В Сочи открылся железнодорожный форум "Стратегическое партнерство 1520"', 'admin', '\"Пространство 1520\" - это территория с единой шириной железнодорожной колеи в 1520 мм, которая включает страны СНГ, Балтии, Финляндия и Монголия.<br>Свое выступление он посвятил перспективам развития бизнеса и ускорения экономического роста железных дорог с шириной колеи 1520 мм.<br>', '1');

