INSERT INTO `news_news` (`title` , `editor` , `text` , `folder_id`  )
VALUES ( 'В память людей, умерших от СПИДа, отслужат панихиды и посадят деревья (Алтайский край)', 'admin', 'На 1 мая 2006 года в России зарегистрировано 357376 ВИЧ-инфицированных, умерло от СПИДа 1306 человек.<br>В Алтайском крае зарегистрировано более 4000 ВИЧ-инфицированных, умерло - 23 человека, - сообщили корреспонденту ИА REGNUM в Алтайском краевом центре СПИД.<br>', '1');

